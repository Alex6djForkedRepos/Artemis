                    xmlns:nodes="clr-namespace:Artemis.VisualScripting.Nodes">

    <DataTemplate x:Key="StaticValueCustomViewTemplate"
                  DataType="{x:Type nodes:StaticIntegerValueNodeCustomViewModel}">
